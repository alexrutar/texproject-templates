  eprint = {2008.00197},
  eprinttype = {arxiv},
  file = {/Users/alexrutar/Database/Zotero/Rutar_2020_Geometric and Combinatorial Properties of Self-similar Multifractal Measures.pdf;/Users/alexrutar/.local/share/zotero/storage/YYUD2L5K/2008.html},
