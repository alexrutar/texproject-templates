% dimension macros
\RequirePackage{amsmath}
\DeclareMathOperator{\dimH}{\operatorname{dim}_{\mathrm{H}}}
\DeclareMathOperator{\dimB}{\operatorname{dim}_{\mathrm{B}}}
\DeclareMathOperator{\dimlB}{\underline{\operatorname{dim}}_{\mathrm{B}}}
\DeclareMathOperator{\dimuB}{\overline{\operatorname{dim}}_{\mathrm{B}}}
\DeclareMathOperator{\dimA}{\operatorname{dim}_{\mathrm{A}}}
\DeclareMathOperator{\dimL}{\operatorname{dim}_{\mathrm{L}}}

\DeclareMathOperator{\loc}{loc}

\DeclareMathOperator{\dimqA}{\operatorname{dim}_{\mathrm{qA}}}
