@article{hoc2017,
  author = {Hochman, Michael},
  year = {2017},
  month = jun,
  journal = {arXiv:1503.09043 [math]},
  eprint = {1503.09043},
  eprinttype = {arxiv},
  primaryclass = {math},
  abstract = {We study self-similar sets and measures on \$\textbackslash mathbb\{R\}\^\{d\}\$. Assuming that the defining iterated function system \$\textbackslash Phi\$ does not preserve a proper affine subspace, we show that one of the following holds: (1) the dimension is equal to the trivial bound (the minimum of \$d\$ and the similarity dimension \$s\$); (2) for all large \$n\$ there are \$n\$-fold compositions of maps from \$\textbackslash Phi\$ which are super-exponentially close in \$n\$; (3) there is a non-trivial linear subspace of \$\textbackslash mathbb\{R\}\^\{d\}\$ that is preserved by the linearization of \$\textbackslash Phi\$ and whose translates typically meet the set or measure in full dimension. In particular, when the linearization of \$\textbackslash Phi\$ acts irreducibly on \$\textbackslash mathbb\{R\}\^\{d\}\$, either the dimension is equal to \$\textbackslash min\textbackslash\{s,d\textbackslash\}\$ or there are super-exponentially close \$n\$-fold compositions. We give a number of applications to algebraic systems, parametrized systems, and to some classical examples. The main ingredient in the proof is an inverse theorem for the entropy growth of convolutions of measures on \$\textbackslash mathbb\{R\}\^\{d\}\$, and the growth of entropy for the convolution of a measure on the orthogonal group with a measure on \$\textbackslash mathbb\{R\}\^\{d\}\$. More generally, this part of the paper applies to smooth actions of Lie groups on manifolds.},
  archiveprefix = {arXiv},
  keywords = {28A80; 11K55; 11B30; 11P70,Mathematics - Classical Analysis and ODEs,Mathematics - Metric Geometry},
  title = {On Self-Similar Sets with Overlaps and Inverse Theorems for Entropy in {$\mathbb{R}^d$}}
}

