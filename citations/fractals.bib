@article{bak2019a,
  title = {Iterated Function Systems with Super-Exponentially Close Cylinders},
  author = {Baker, Simon},
  year = {2019},
  month = sep,
  journal = {arXiv:1909.04343 [math]},
  eprint = {1909.04343},
  eprinttype = {arxiv},
  primaryclass = {math},
  abstract = {Several important conjectures in Fractal Geometry can be summarised as follows: If the dimension of a self-similar measure in \$\textbackslash mathbb\{R\}\$ does not equal its expected value, then the underlying iterated function system contains an exact overlap. In recent years significant progress has been made towards these conjectures. Hochman proved that if the Hausdorff dimension of a self-similar measure in \$\textbackslash mathbb\{R\}\$ does not equal its expected value, then there are cylinders which are super-exponentially close at all small scales. Several years later, Shmerkin proved an analogous statement for the \$L\^q\$ dimension of self-similar measures in \$\textbackslash mathbb\{R\}\$. With these statements in mind, it is natural to wonder whether there exist iterated function systems that do not contain exact overlaps, yet there are cylinders which are super-exponentially close at all small scales. In this paper we show that such iterated function systems do exist. In fact we prove much more. We prove that for any sequence \$(\textbackslash epsilon\_n)\_\{n=1\}\^\{\textbackslash infty\}\$ of positive real numbers, there exists an iterated function system \$\textbackslash\{\textbackslash phi\_i\textbackslash\}\_\{i\textbackslash in \textbackslash mathcal\{I\}\}\$ that does not contain exact overlaps and \$\$\textbackslash min\textbackslash left\textbackslash\{|\textbackslash phi\_\{\textbackslash mathbf\{a\}\}(0)-\textbackslash phi\_\{\textbackslash mathbf\{b\}\}(0)|: \textbackslash mathbf\{a\},\textbackslash mathbf\{b\}\textbackslash in \textbackslash mathcal\{I\}\^n,\textbackslash, \textbackslash mathbf\{a\}\textbackslash neq \textbackslash mathbf\{b\},\textbackslash, r\_\{\textbackslash mathbf\{a\}\}=r\_\{\textbackslash mathbf\{b\}\}\textbackslash right\textbackslash\}\textbackslash leq \textbackslash epsilon\_n\$\$ for all \$n\textbackslash in \textbackslash mathbb\{N\}.\$},
  archiveprefix = {arXiv},
  keywords = {28A80; 37C45,Mathematics - Classical Analysis and ODEs,Mathematics - Dynamical Systems,Mathematics - Metric Geometry}
}

@article{bak2020,
  ids = {bak2019},
  title = {Overlapping Iterated Function Systems from the Perspective of {{Metric Number Theory}}},
  author = {Baker, Simon},
  year = {2020},
  month = oct,
  journal = {arXiv:1901.07875 [math]},
  eprint = {1901.07875},
  eprinttype = {arxiv},
  primaryclass = {math},
  abstract = {In this paper we develop a new approach for studying overlapping iterated function systems. This approach is inspired by a famous result due to Khintchine from Diophantine approximation. This result shows that for a family of limsup sets, their Lebesgue measure is determined by the convergence or divergence of naturally occurring volume sums. For many parameterised families of overlapping iterated function systems, we prove that a typical member will exhibit similar Khintchine like behaviour. Families of iterated function systems our results apply to include those arising from Bernoulli convolutions, the \$\textbackslash\{0,1,3\textbackslash\}\$ problem, and affine contractions with varying translation parameter. As a by-product of our analysis we obtain new proofs of well known results due to Solomyak on the absolute continuity of Bernoulli convolutions, and when the attractor in the \$\textbackslash\{0,1,3\textbackslash\}\$ problem has positive Lebesgue measure. For each \$t\textbackslash in [0,1]\$ we let \$\textbackslash Phi\_t\$ be the iterated function system given by \$\$\textbackslash Phi\_\{t\}:=\textbackslash Big\textbackslash\{\textbackslash phi\_1(x)=\textbackslash frac\{x\}\{2\},\textbackslash phi\_2(x)=\textbackslash frac\{x+1\}\{2\},\textbackslash phi\_3(x)=\textbackslash frac\{x+t\}\{2\},\textbackslash phi\_\{4\}(x)=\textbackslash frac\{x+1+t\}\{2\}\textbackslash Big\textbackslash\}.\$\$ We include a detailed study of this family. We prove that either \$\textbackslash Phi\_t\$ contains an exact overlap, or we observe Khintchine like behaviour. Our analysis of this family shows that by studying the metric properties of limsup sets, we can distinguish between the overlapping behaviour of iterated function systems in a way that is not available to us by simply studying properties of self-similar measures. Last of all, we introduce a property of an iterated function system that we call being consistently separated with respect to a measure. We prove that this property implies that the pushforward of the measure is absolutely continuous. We include several explicit examples of consistently separated iterated function systems.},
  archiveprefix = {arXiv},
  keywords = {Mathematics - Classical Analysis and ODEs,Mathematics - Dynamical Systems,Mathematics - Number Theory}
}

@article{ban2020,
  title = {Generalised Intermediate Dimensions},
  author = {Banaji, Amlan},
  year = {2020},
  month = nov,
  journal = {arXiv:2011.08613 [math]},
  eprint = {2011.08613},
  eprinttype = {arxiv},
  primaryclass = {math},
  abstract = {We introduce a family of dimensions, which we call the \$\textbackslash Phi\$-intermediate dimensions, that lie between the Hausdorff and box dimensions and generalise the intermediate dimensions introduced by Falconer, Fraser and Kempton. We do this by restricting the allowable covers in the definition of Hausdorff dimension, but in a wider variety of ways than in the definition of the intermediate dimensions. We also extend the theory from Euclidean space to a wider class of metric spaces. We investigate relationships between the \$\textbackslash Phi\$-intermediate dimensions and other notions of dimension, and study many analytic and geometric properties of the dimensions. We prove continuity-like results which improve similar results for the intermediate dimensions and give a sharp general lower bound for the intermediate dimensions that is positive for all \$\textbackslash theta \textbackslash in (0,1]\$ for sets with positive box dimension. We prove H\textbackslash "older distortion estimates which imply bi-Lipschitz stability for the \$\textbackslash Phi\$-intermediate dimensions. We prove a mass distribution principle and Frostman type lemma, and use these to study dimensions of product sets, and to show that the lower versions of the dimensions, unlike the the upper versions, are not finitely stable. We show that for any compact subset of an appropriate space, these dimensions can be used to `recover the interpolation' between the Hausdorff and box dimensions of sets for which the intermediate dimensions are discontinuous at \$\textbackslash theta=0\$, thus providing more refined geometric information about such sets.},
  archiveprefix = {arXiv},
  keywords = {28A80 (Primary); 28A78 (Secondary),Mathematics - Classical Analysis and ODEs,Mathematics - Metric Geometry}
}

