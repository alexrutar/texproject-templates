  issn = {0305-0041, 1469-8064},
  doi = {10.1017/S0305004100064926},
  abstract = {If \$T\$ is a linear transformation on \$\textbackslash mathbb\{R\}\^n\$ with singular values \$\textbackslash alpha\_1\textbackslash geq\textbackslash alpha\_2\textbackslash geq\textbackslash ldots\textbackslash geq\textbackslash alpha\_n\$, the singular value function \$\textbackslash phi\^s\$ is defined by \$\textbackslash phi\^s(T)=\textbackslash alpha\_1\textbackslash alpha\_2\textbackslash ldots\textbackslash alpha\_\{m-1\}\textbackslash alpha\_m\^\{s-m+1\}\$ where \$m\$ is the smallest integer greater than or equal to \$s\$. Let \$T\_1,\textbackslash ldots,T\_k\$ be contractive linear transformations on \$\textbackslash mathbb\{R\}\^n\$. Let \$d=\textbackslash inf\textbackslash\{s\textbackslash sum\textbackslash phi\^s(T\_\{i\_1\}T\_\{i\_2\}\textbackslash ldots T\_\{i\_r\}){$<\backslash$}infty\textbackslash\}\$ where the sum is over all finite sequences \$(i\_1,\textbackslash ldots,i\_r)\$ with \$1\textbackslash leq i\_j\textbackslash leq k\$. Then for almost all \$(a\_1,\textbackslash ldots,a\_k)\textbackslash in\textbackslash R\^\{nk\}\$, the unique non-empty compact set \$F\$ satisfying \$F=\textbackslash bigcup\_\{i=1\}\^k (T\_i(F)+a\_i)\$ has Hausdorff dimension \$\textbackslash min\textbackslash\{d, n\textbackslash\}\$. Moreover the `box counting' dimension of \$F\$ is almost surely equal to this number.},
  langid = {english}
}

