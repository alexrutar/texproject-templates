@article{fen2020,
  title = {Dimension of Invariant Measures for Affine Iterated Function Systems},
  author = {Feng, De-Jun},
  year = {2020},
  month = jun,
  journal = {arXiv:1901.01691 [math]},
  eprint = {1901.01691},
  eprinttype = {arxiv},
  primaryclass = {math},
  abstract = {Let \$\textbackslash\{S\_i\textbackslash\}\_\{i\textbackslash in \textbackslash Lambda\}\$ be a finite contracting affine iterated function system (IFS) on \$\{\textbackslash Bbb R\}\^d\$. Let \$(\textbackslash Sigma,\textbackslash sigma)\$ denote the two-sided full shift over the alphabet \$\textbackslash Lambda\$, and \$\textbackslash pi:\textbackslash Sigma\textbackslash to \{\textbackslash Bbb R\}\^d\$ be the coding map associated with the IFS. We prove that the projection of an ergodic \$\textbackslash sigma\$-invariant measure on \$\textbackslash Sigma\$ under \$\textbackslash pi\$ is always exact dimensional, and its Hausdorff dimension satisfies a Ledrappier-Young type formula. Furthermore, the result extends to average contracting affine IFSs. This completes several previous results and answers a folklore open question in the community of fractals. Some applications are given to the dimension of self-affine sets and measures.},
  archiveprefix = {arXiv},
  keywords = {Mathematics - Classical Analysis and ODEs,Mathematics - Dynamical Systems}
}

@article{ffk2020,
  title = {Intermediate Dimensions},
  author = {Falconer, Kenneth J. and Fraser, Jonathan M. and Kempton, Tom},
  year = {2020},
  month = oct,
  journal = {Math. Z.},
  volume = {296},
  number = {1-2},
  pages = {813--830},
  issn = {0025-5874, 1432-1823},
  doi = {10.1007/s00209-019-02452-0},
  abstract = {We introduce a continuum of dimensions which are `intermediate' between the familiar Hausdorff and box dimensions. This is done by restricting the families of allowable covers in the definition of Hausdorff dimension by insisting that \$|U| \textbackslash le |V|\^\textbackslash theta \$ for all sets \$U\$, \$V\$ used in a particular cover, where \$\textbackslash theta \textbackslash in [0,1]\$ is a parameter. Thus, when \$\textbackslash theta =1\$ only covers using sets of the same size are allowable, and we recover the box dimensions, and when \$\textbackslash theta =0\$ there are no restrictions, and we recover Hausdorff dimension. We investigate many properties of the intermediate dimension (as a function of \$\textbackslash theta \$), including proving that it is continuous on \$(0, 1]\$ but not necessarily continuous at 0, as well as establishing appropriate analogues of the mass distribution principle, Frostman's lemma, and the dimension formulae for products. We also compute, or estimate, the intermediate dimensions of some familiar sets, including sequences formed by negative powers of integers, and Bedford\textendash McMullen carpets.},
  langid = {english}
}

