@article{rutpreprinta,
  title = {A {{Multifractal Decomposition}} for {{Self-similar Measures}} with {{Exact Overlaps}}},
  author = {Rutar, Alex},
  year = {preprint},
  journal = {arXiv:2104.06997},
  eprint = {2104.06997},
  eprinttype = {arxiv},
  abstract = {We study self-similar measures in \$\textbackslash mathbb\{R\}\$ satisfying the weak separation condition along with weak technical assumptions which are satisfied in all known examples. For such a measure \$\textbackslash mu\$, we show that there is a finite set of concave functions \$\textbackslash\{\textbackslash tau\_1,\textbackslash ldots,\textbackslash tau\_m\textbackslash\}\$ such that the \$L\^q\$-spectrum of \$\textbackslash mu\$ is given by \$\textbackslash min\textbackslash\{\textbackslash tau\_1,\textbackslash ldots,\textbackslash tau\_m\textbackslash\}\$ and the multifractal spectrum of \$\textbackslash mu\$ is given by \$\textbackslash max\textbackslash\{\textbackslash tau\_1\^*,\textbackslash ldots,\textbackslash tau\_m\^*\textbackslash\}\$, where \$\textbackslash tau\_i\^*\$ denotes the concave conjugate of \$\textbackslash tau\_i\$. In particular, the measure \$\textbackslash mu\$ satisfies the multifractal formalism if and only if its multifractal spectrum is a concave function. This implies that \$\textbackslash mu\$ satisfies the multifractal formalism at values corresponding to points of differentiability of the \$L\^q\$-spectrum. We also verify existence of the limit for the \$L\^q\$-spectra of such measures for every \$q\textbackslash in\textbackslash mathbb\{R\}\$. As a direct application, we obtain many new results and simple proofs of well-known results in the multifractal analysis of self-similar measures satisfying the weak separation condition.},
  archiveprefix = {arXiv},
  keywords = {28A78 (Primary) 28A80 (Secondary),Mathematics - Dynamical Systems}
}

@article{ruttoappear,
